<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>ParticipantServlet</servlet-name>
  	<display-name>ParticipantServlet</display-name>
  	<description></description>
  	<servlet-class>com.courseendprojects.controller.ParticipantServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>BatchServlet</servlet-name>
  	<display-name>BatchServlet</display-name>
  	<description></description>
  	<servlet-class>com.courseendprojects.controller.BatchServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ParticipantServlet</servlet-name>
  	<url-pattern>/ParticipantServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>BatchServlet</servlet-name>
  	<url-pattern>/BatchServlet</url-pattern>
  </servlet-mapping>
</web-app>
